{
  "name": "davisenra/json-parser",
  "description": "Just a JSON parser.",
  "type": "library",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "JsonParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Davi Senra"
    }
  ],
  "scripts": {
    "format": "vendor/bin/mago format src tests"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "symfony/var-dumper": "^7.3",
    "carthage-software/mago": "^1.0.0-beta.26"
  },
  "config": {
    "allow-plugins": {
      "carthage-software/mago": true
    }
  }
}
